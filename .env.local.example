# Copy this file to `.env` to run the orchestrator locally without Google Cloud.
# Only adjust the values below to your preference.

LOCAL_MODE=1
PROJECT_ID=local-demo
PORT=8080

# REGION=us-central1
# GEMINI_API_KEY=your-gemini-api-key
# GOOGLE_APPLICATION_CREDENTIALS="path/to/your/service-account-file.json"
# GEMINI_MODEL=gemini-2.5-pro

# Optional model overrides
# MODEL_RESEARCH=gemini-1.5-flash
# MODEL_ANALYSIS=gemini-1.5-pro
# MODEL_CODE=gemini-2.0-pro
# MODEL_VALIDATOR=gemini-1.5-pro

# Optional storage bucket for packaged artifacts (only in cloud deployments)
# PACKAGE_ARCHIVE_BUCKET=your-package-bucket-name
